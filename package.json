    "static-reference": "^1.0.0",
    "stream-sequencer": "^0.3.1"
  },
  "devDependencies": {
    "browserify": "^5.12.1",
    "grunt": "^0.4.5",
    "grunt-browserify": "^3.0.1",
    "grunt-contrib-compress": "^0.12.0",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-contrib-less": "^0.11.4",
    "stylerefs": "^0.1.6"
  },
  "jshintConfig": {
    "node": true,
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "immed": true,
    "indent": 8,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "nonbsp": true,
    "nonew": true,
    "quotmark": "single",
    "undef": true,
    "unused": "vars",
    "strict": true,
    "laxbreak": true,
    "predef": [
      "-Map"
    ],
    "-W087": true
  }
}
